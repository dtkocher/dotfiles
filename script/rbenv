# Install rbenv tools
brew install rbenv
brew install rbenv-gemset
brew install ruby-build

# Install versions
rbenv install 1.9.3-p547
rbenv global 1.9.3-p547
rbenv rehash

rbenv install 2.0.0-p576
rbenv rehash

rbenv install 2.1.5
rbenv rehash

rbenv install 2.2.1
rbenv rehash

rbenv rehash
rbenv install jruby-1.7.16

