# Uninstall these apps and corresponding files
brew cask zap google-drive
brew cask zap google-chrome
brew cask zap google-hangouts
rm -rf ~/Google\ Drive/
rm -rf /Library/Application\ Support/Google/
rm -rf /Library/Google/

brew cask zap dropbox
rm -rf ~/Dropbox/
rm -rf ~/.dropbox/
rm -rf /Library/DropboxHelperTools/

brew cask zap virtualbox
rm -rf ~/VirtualBox\ VMs/

# Clean up cached files
brew cask cleanup
brew cleanup

