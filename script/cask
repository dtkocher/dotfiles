# Install brew-cask will allow you to install several apps not
# provided by the app store
brew install caskroom/cask/brew-cask

# Install desired apps
brew install java
brew install iterm2
brew install ccleaner
brew install dropbox
brew install 1password
brew install nvalt
brew install openoffice
brew install virtualbox
brew install gitx
brew install github
brew install avast
brew install heroku-toolbelt
brew install visual-studio-code
brew install psequel


# Clean up cached install files
brew cleanup
